{
  "name": "telegram-bot",
  "module": "main.ts",
  "type": "module",
  "scripts": {
    "DBCreate": "drizzle-kit generate:sqlite --config drizzle.config.ts",
    "DBMigrate": "bun ./src/migrate.ts",
    "start": "bun ./src/main.ts",
    "lint": "bunx oxlint && tsc ",
    "saveStatsResults": "bun ./src/backtest/services/saveStatsResults.ts",
    "showStatsResults": "bun ./src/backtest/services/showStatsResults.ts",
    "saveBacktestData": "bun ./src/backtest/services/saveBacktestData.ts",
    "showBacktestData": "bun ./src/backtest/services/showBacktestData.ts",
    "backtestAcc": "bun ./src/backtest/accumulate.ts",
    "DBDelete": "rm -rf ./data && rm -rf ./drizzle && rm -rf ./node_modules",
    "DBRestart": "bun DBDelete && bun i && bun DBCreate && bun DBMigrate "
  },
  "devDependencies": {
    "@types/bun": "latest",
    "drizzle-kit": "^0.20.14"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@types/big.js": "^6.2.2",
    "@types/cli-progress": "^3.11.5",
    "axios": "^1.6.8",
    "big.js": "^6.2.1",
    "binance-api-node": "^0.12.7",
    "cli-progress": "^3.12.0",
    "drizzle-orm": "^0.30.4",
    "technicalindicators": "^3.1.0"
  }
}